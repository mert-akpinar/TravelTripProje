@using TravelTripProje.Models.Classes
@model List<Blog>
@foreach (var x in Model)
{
<div class="box1-left">
    <div class="bot-img">
        <img src="@x.BlogImageUrl" style="width: 130px; height:75px;" alt="" />
    </div>
    <div class="bot-desc1">
        <p>@x.Description.Substring(0,75)...</p>
    </div>
    <div class="clear"></div>
</div>
}