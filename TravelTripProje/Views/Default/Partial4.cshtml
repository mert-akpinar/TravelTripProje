@using TravelTripProje.Models.Classes
@model List<Blog>
<div class="padding1">
    <h4>En Beğendiğim Yerler</h4>
    <div class="box-left">
        @foreach (var x in Model)
        {
            <div class="bot-img">
                <img src="@x.BlogImageUrl" style="width: 130px; height:75px;" alt="" />
            </div>
            <div class="bot-desc">
                <p>@x.Description.Substring(0,75)...</p>
            </div>
            <div class="clear"></div>
        }
    </div>
    @Html.Action("Partial5","Default")
</div>
